{
    "manifest_version": 2,
    "name": "Simple Scraper",
    "version": "1.0",
    "description": "Simple Scraper",
    "browser_action": {
        "default_title": "Simple Scraper",
        "default_popup": "popout.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"],
        "run_at": "document_end"
    }]
    
}